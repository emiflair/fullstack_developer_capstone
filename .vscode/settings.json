{
  "files.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/*.pyc": true,
    "**/__pycache__": true,
    "**/build": true,
    "**/dist": true,
    "**/static/admin/**": true,
    "**/static/css/**": true,
    "**/static/js/**": true,
    "**/static/static/**": true,
    "**/static/*.min.css": true,
    "**/static/*.min.js": true,
    "**/database/node_modules/**": true,
    "**/server/database/node_modules/**": true,
    "**/frontend/node_modules/**": true,
    "**/.github/**": true,
    "**/tsconfig.json": true,
    "**/FUNDING.yml": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/build": true,
    "**/.git": true,
    "**/dist": true,
    "**/*.min.js": true,
    "**/*.min.css": true,
    "**/static/admin/**": true,
    "**/static/css/**": true,
    "**/static/js/**": true,
    "**/static/static/**": true,
    "**/database/node_modules/**": true,
    "**/server/database/node_modules/**": true,
    "**/frontend/node_modules/**": true,
    "**/.github/**": true,
    "**/tsconfig.json": true,
    "**/FUNDING.yml": true
  },
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/.git/**": true,
    "**/build/**": true,
    "**/dist/**": true,
    "**/*.pyc": true,
    "**/__pycache__/**": true,
    "**/static/admin/**": true,
    "**/static/css/**": true,
    "**/static/js/**": true,
    "**/static/static/**": true,
    "**/database/node_modules/**": true,
    "**/server/database/node_modules/**": true,
    "**/frontend/node_modules/**": true,
    "**/.github/**": true,
    "**/tsconfig.json": true,
    "**/FUNDING.yml": true
  },
  "typescript.validate.enable": false,
  "typescript.preferences.includePackageJsonAutoImports": "off",
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.suggest.autoImports": false,
  "typescript.preferences.useAliasesForRenames": false,
  "typescript.surveys.enabled": false,
  "javascript.validate.enable": false,
  "javascript.suggest.autoImports": false,
  "json.validate.enable": false,
  "json.schemaDownload.enable": false,
  "yaml.validate": false,
  "yaml.format.enable": false,
  "yaml.schemaStore.enable": false,
  "html.validate.styles": false,
  "html.validate.scripts": false,
  "css.validate": false,
  "scss.validate": false,
  "less.validate": false,
  "eslint.enable": false,
  "tslint.enable": false,
  "python.linting.enabled": false,
  "html.autoClosingTags": false,
  "html.completion.attributeDefaultValue": "empty",
  "emmet.showExpandedAbbreviation": "never",
  "css.completion.completePropertyWithSemicolon": false,
  "css.completion.triggerPropertyValueCompletion": false,
  "html.format.enable": false,
  "css.format.enable": false,
  "json.format.enable": false,
  "javascript.format.enable": false,
  "typescript.format.enable": false,
  "files.associations": {
    "*.css": "plaintext",
    "*.min.css": "plaintext",
    "*.min.js": "plaintext",
    "tsconfig.json": "plaintext",
    "FUNDING.yml": "plaintext",
    "**/node_modules/**/*.json": "plaintext",
    "**/node_modules/**/*.yml": "plaintext",
    "**/node_modules/**/*.yaml": "plaintext",
    "**/node_modules/**/*.css": "plaintext",
    "**/node_modules/**/*.js": "plaintext",
    "**/node_modules/**/*.ts": "plaintext",
    "**/static/**/*.css": "plaintext",
    "**/static/**/*.js": "plaintext",
    "**/static/**/*.html": "plaintext",
    "*.min.*": "plaintext",
    "**/.github/**": "plaintext"
  },
  "workbench.editor.enablePreview": false,
  "problems.showCurrentInStatus": false,
  "problems.decorations.enabled": false,
  "editor.renderValidationDecorations": "off",
  "editor.semanticHighlighting.enabled": false,
  "editor.hover.enabled": false,
  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": false
  },
  "editor.parameterHints.enabled": false,
  "editor.suggest.showStatusBar": false,
  "workbench.colorCustomizations": {
    "editorError.foreground": "#00000000",
    "editorWarning.foreground": "#00000000",
    "editorInfo.foreground": "#00000000",
    "problemsErrorIcon.foreground": "#00000000",
    "problemsWarningIcon.foreground": "#00000000",
    "problemsInfoIcon.foreground": "#00000000"
  },
  "editor.rulers": [],
  "breadcrumbs.enabled": false,
  "editor.codeLens": false,
  "editor.inlayHints.enabled": "off",
  "extensions.ignoreRecommendations": true,
  "problems.visible": false,
  "outline.showClasses": false,
  "outline.showConstants": false,
  "outline.showConstructors": false,
  "outline.showEnumMembers": false,
  "outline.showEnums": false,
  "outline.showEvents": false,
  "outline.showFields": false,
  "outline.showFiles": false,
  "outline.showFunctions": false,
  "outline.showInterfaces": false,
  "outline.showKeys": false,
  "outline.showMethods": false,
  "outline.showModules": false,
  "outline.showNamespaces": false,
  "outline.showNull": false,
  "outline.showNumbers": false,
  "outline.showObjects": false,
  "outline.showOperators": false,
  "outline.showPackages": false,
  "outline.showProperties": false,
  "outline.showStrings": false,
  "outline.showStructs": false,
  "outline.showTypeParameters": false,
  "outline.showVariables": false,
  "css.lint.duplicateProperties": "ignore",
  "css.lint.emptyRules": "ignore",
  "css.lint.importStatement": "ignore",
  "css.lint.boxModel": "ignore",
  "css.lint.universalSelector": "ignore",
  "css.lint.zeroUnits": "ignore",
  "css.lint.fontFaceProperties": "ignore",
  "css.lint.hexColorLength": "ignore",
  "css.lint.argumentsInColorFunction": "ignore",
  "css.lint.unknownProperties": "ignore",
  "css.lint.validProperties": [],
  "html.validate.styles": false,
  "html.validate.scripts": false,
  "html.format.wrapAttributes": "preserve"
}
